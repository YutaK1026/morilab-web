---
export interface Props {
  labNameJp: string;
  labNameEn: string;
}
const { labNameJp, labNameEn } = Astro.props;
---
<section class="full-hero-image">
  <img src="/top/top_image.jpg" alt="Lab Top" class="hero-bg-img" />
  <div class="hero-overlay"></div>
  <div class="hero-text-block">
    <div class="hero-lab-jp">{labNameJp}</div>
    <div class="hero-lab-en">{labNameEn}</div>
  </div>
</section>